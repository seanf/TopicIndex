<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/TagToTagList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{tagToTagHome.wire}"/>

   <param name="tagToTagFrom"/>
   <param name="tagToTagTagToTagId" value="#{tagToTagHome.tagToTagTagToTagId}"/>
   <param name="tagToTagRelationshipFrom"/>
   <param name="tagToTagRelationshipTagToTagRelationshipType" value="#{tagToTagRelationshipHome.tagToTagRelationshipTagToTagRelationshipType}"/>



   <navigation from-action="#{tagToTagHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/TagToTag.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tagToTagHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/TagToTag.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tagToTagHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/TagToTagList.xhtml"/>
      </rule>
   </navigation>

</page>
