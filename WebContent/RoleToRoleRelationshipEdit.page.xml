<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/RoleToRoleRelationshipList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{roleToRoleRelationshipHome.wire}"/>

   <param name="roleToRoleRelationshipFrom"/>
   <param name="roleToRoleRelationshipRoleToRoleRelationshipId" value="#{roleToRoleRelationshipHome.roleToRoleRelationshipRoleToRoleRelationshipId}"/>


   <navigation from-action="#{roleToRoleRelationshipHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/RoleToRoleRelationship.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{roleToRoleRelationshipHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/RoleToRoleRelationship.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{roleToRoleRelationshipHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/RoleToRoleRelationshipList.xhtml"/>
      </rule>
   </navigation>

</page>
