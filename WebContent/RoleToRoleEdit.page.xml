<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/RoleToRoleList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{roleToRoleHome.wire}"/>

   <param name="roleToRoleFrom"/>
   <param name="roleToRoleRoleToRoleId" value="#{roleToRoleHome.roleToRoleRoleToRoleId}"/>
   <param name="roleToRoleRelationshipFrom"/>
   <param name="roleToRoleRelationshipRoleToRoleRelationshipId" value="#{roleToRoleRelationshipHome.roleToRoleRelationshipRoleToRoleRelationshipId}"/>



   <navigation from-action="#{roleToRoleHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/RoleToRole.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{roleToRoleHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/RoleToRole.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{roleToRoleHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/RoleToRoleList.xhtml"/>
      </rule>
   </navigation>

</page>
