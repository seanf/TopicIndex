<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/TopicToTopicSourceUrlList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{topicToTopicSourceUrlHome.wire}"/>

   <param name="topicToTopicSourceUrlFrom"/>
   <param name="topicToTopicSourceUrlTopicToTopicSourceUrlid" value="#{topicToTopicSourceUrlHome.topicToTopicSourceUrlTopicToTopicSourceUrlid}"/>
   <param name="topicSourceUrlFrom"/>
   <param name="topicSourceUrlTopicSourceUrlid" value="#{topicSourceUrlHome.topicSourceUrlTopicSourceUrlid}"/>



   <navigation from-action="#{topicToTopicSourceUrlHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/TopicToTopicSourceUrl.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{topicToTopicSourceUrlHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/TopicToTopicSourceUrl.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{topicToTopicSourceUrlHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/TopicToTopicSourceUrlList.xhtml"/>
      </rule>
   </navigation>

</page>
